<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="LightBlue">#FF64A5B4"</Color>
    <Color x:Key="MediumBlue">#FF00A8B4"</Color>
    <Color x:Key="DarkBlue">#FF1D5062"</Color>
    <Color x:Key="Gray">#FF6B6B6B"</Color>
    <Color x:Key="Green">#FFA3D10D"</Color>

    <SolidColorBrush x:Key="LightBlueBrush"
                     Color="{StaticResource LightBlue}" />
    <SolidColorBrush x:Key="MediumBlueBrush"
                     Color="{StaticResource MediumBlue}" />
    <SolidColorBrush x:Key="DarkBlueBrush"
                     Color="{StaticResource DarkBlue}" />
    <SolidColorBrush x:Key="GrayBrush"
                     Color="{StaticResource Gray}" />
</ResourceDictionary>
